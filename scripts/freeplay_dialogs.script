--' TRADER CHECK REWARD

function trader_check_reward()
     for i = 1,6 do
        if has_alife_info("trader_freeplay_reward_task_"..i.."_start") and not has_alife_info("trader_freeplay_reward_task_"..i.."_done") then
            return true
        end  
     end
     return false
end
function trader_not_check_reward()
    return not trader_check_reward()
end


