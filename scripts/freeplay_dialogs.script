--' TRADER GIVE REWARD
    
function trader_give_reward(first_speaker, second_speaker)
    local spawn_items = {
        [1] = {"outfit_novice_m1","treasure_item","vodka","conserva"},
        [2] = {"outfit_bandit_m1","treasure_item","vodka","conserva"},
        [3] = {"outfit_stalker_m1","treasure_item","vodka","conserva"},
        [4] = {"treasure_item","vodka","conserva"},
        [5] = {"treasure_item","vodka","conserva"},
        [6] = {"treasure_item","vodka","conserva"}
       }    
    for i = 1,6 do 
         if has_alife_info("trader_freeplay_reward_task_"..i.."_start") and not has_alife_info("trader_freeplay_reward_task_"..i.."_done") then
           db.actor:give_info_portion("trader_freeplay_reward_task_"..i.."_done")
            for k,v in pairs(spawn_items[i]) do
                if type(tonumber(v)) == "number" then
                    db.actor:give_money(tonumber(v))
                else
                    alife():create(v,	
						db.actor:position(),
						db.actor:level_vertex_id(),	
						db.actor:game_vertex_id(), 
						db.actor:id())
                    news_manager.relocate_item(db.actor, "in", v)
                end
            end
			break
        end
    end   
end 


