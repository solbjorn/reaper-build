function load_states(type)
	if type == "amk_embankment" then
		return function(gulag)
			if Actor ~= nil then
				if ( level.get_time_hours() >= 5 and level.get_time_hours() <= 22 ) then
					return 0
				else
					return 1
				end
			else
				return gulag.state
			end
		end
	end
	if type == "amk_mini_bar" then
		return function(gulag)
			if Actor ~= nil then
				if ( level.get_time_hours() >= 5 and level.get_time_hours() <= 22 ) then
					return 0
				else
					return 1
				end
			else
				return gulag.state
			end
		end
	end
end

function checkStalker(npc_community, gulag_type)
	if gulag_type == "amk_embankment" then
		return npc_community == "military"
	end
	if gulag_type == "amk_mini_bar" then
		return npc_community == "stalker"
	end
	return false
end

