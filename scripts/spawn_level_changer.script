function on_item_drop(obj)
	local obj_sect = obj:section()
	local teleport = teleports[obj_sect]
	if not teleport then return end -- не наш "клиент"

	-- если арт не нужен или он есть у ГГ
	if not teleport.art or db_actor:object(teleport.art) then
		-- отправляем телепорт на экспертизу
		-- узнаем не съели ли его или выложили в нычку
		local tbl = {
			obj_id		= obj:id(),
			obj_sect	= obj_sect
		}
		amk.start_timer("timer_drop_obj_teleport", 0.2, amk.pack_array_to_string(tbl))
	end
end

function teleport_start(obj_sect)
	local teleport = teleports[obj_sect]

	-- отбираем арт у ГГ, если нужно отобрать
	if teleport.art then
		local art = db_actor:object(teleport.art)
		if art then
			local sim = alife()
			local se_art = sim:object(art:id())
			if se_art then
				-- вызываем случайную функцию для данного телепорта
				jump(teleport.goto_func[math.random(#teleport.goto_func)])
				sim:release(se_art)
			end
		end
	else
		jump(teleport.goto_func[math.random(#teleport.goto_func)])
	end

	if teleport.keep then -- вернуть телепорт в рюкзак
		amk.spawn_item_in_inv(obj_sect)
	end
end

function remove_old_teleport()
	local sim = alife()
	local se_obj
	for nam, tbl in pairs(places) do
		se_obj = sim:story_object(tbl.sid)
		if se_obj then
			sim:release(se_obj, true)
		end
	end
end
