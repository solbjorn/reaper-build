local spot_type = { "ogsm_hide_spot_min", "ogsm_hide_spot_mdl", "ogsm_hide_spot_max" }

function pda_hide_hideouts()
	local spots = get_value("hideout_spots", { }, true)
	local sobj

	for _, spot_id in ipairs(spots) do
		sobj = server_object(spot_id)
		if sobj then
			for i = 1, #spot_type do
				if has_map_spot(sobj.id, spot_type[i]) > 0 then
					del_map_spot(sobj.id, spot_type[i])
				end
			end

			release(sobj)
		end
	end
end

function zone_hideout(t)
	local max_lv = reap_misc.get_max_level_vertex()
	local v2 = vector():set(unpack(t.p3 or t.p2))
	local v1 = vector():set(unpack(t.p1))
	local pos = v1:sub(v2):div(2):add(v2)
	local min_dist = math.huge
	local vdist, lv

	for vv = 1, max_lv do
		vdist = level.vertex_position(vv):distance_to(pos)
		if vdist < min_dist then
			min_dist = vdist
			lv = vv
		end
	end

	return create("space_restrictor", pos, lv, reap_misc.get_nearest_gv(pos))
end

function pda_show_hideouts()
	pda_hide_hideouts()

	local hideouts = spot_info[curr_level]
	if not hideouts or not has_info(hideouts) then return end
	hideouts = amk_hideouts.hide[curr_level]

	local spots = { }
	local pos, zone

	for _, hideout in ipairs(hideouts) do
		if hideout.center then
			pos = level.vertex_position(hideout.center)
			zone = create("space_restrictor", pos, hideout.center,
				      reap_misc.get_nearest_gv(pos))

			map_spot(zone.id, spot_type[hideout.spot or 1], "st_hideout_spot_hint")
			spots[#spots + 1] = zone.id
		else
			for _, zone in ipairs(hideout.zone) do
				zone = zone_hideout(zone)

				map_spot(zone.id, spot_type[hideout.spot or 1], "st_hideout_spot_hint")
				spots[#spots + 1] = zone.id
			end
		end
	end

	set_value("hideout_spots", spots)
end
