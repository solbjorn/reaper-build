blowout_light		= on
light_color		    = 1.4,1.6,1.8
light_range		    = 30.0
light_time 		    = 0.8;0.35
light_height		= 1.5			;подъем источника света на высоту

idle_light			= on
idle_light_range			= 15.0
idle_light_range_delta 		= 0.25
idle_light_anim				= teleport_stancia_in_01
